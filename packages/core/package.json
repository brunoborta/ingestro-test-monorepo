{
  "name": "@ingestro/core",
  "version": "1.0.0",
  "description": "Framework-agnostic import & review SDK",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly --declaration",
    "dev": "tsup --watch",
    "test": "vitest"
  },
  "keywords": [],
  "author": "Bruno 'Borta' Bortagaray",
  "license": "ISC",
  "devDependencies": {
    "tsup": "^8.5.1"
  }
}
