{
  "name": "@borta/core",
  "version": "1.0.1",
  "description": "Framework-agnostic import & review SDK",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "Bruno 'Borta' Bortagaray",
  "license": "ISC",
  "devDependencies": {
    "tsup": "^8.5.1"
  }
}
